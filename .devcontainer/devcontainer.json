{
    "dockerComposeFile": "docker-compose.yml",
    "extensions": [
        "eamodio.gitlens",
    ],
    "portsAttributes": {
        "80": {
            "label": "apache2",
            "onAutoForward": "notify"
        }
    },
    "forwardPorts": [80],
    "postAttachCommand": "sudo service apache2 start",
    "service": "app",
    "workspaceFolder": "/workspace"
}